{
  "name": "user-management",
  "version": "1.0.0",
  "description": "Typescript User Management Logic Layer",
  "main": "dist/cjs/index.js",
  "module": "dist/mjs/index.js",
  "exports": {
    ".": {
      "import": "./dist/mjs/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./feature": {
      "import": "./dist/mjs/interface.js",
      "require": "./dist/cjs/interface.js"
    }
  },
  "scripts": {
    "test:ts": "jest src/test/*.test.ts",
    "test:mjs": "jest dist/mjs/test/*.test.js",
    "test:cjs": "jest dist/cjs/test/*.test.js",
    "lint": "eslint . --fix",
    "build": "rm -fr dist/* && tsc -p tsconfig.json && tsc -p tsconfig-cjs.json && ./afterbuild.sh"
  },
  "directories": {
    "src": "src",
    "test": "test"
  },
  "jest": {
    "transform": {
      "^.+\\.[t|j]sx?$": "babel-jest"
    },
    "testTimeout": 7000
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/preset-env": "^7.24.0",
    "@babel/preset-typescript": "^7.23.3",
    "@jest/globals": "^29.7.0",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "babel-jest": "^29.7.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "typescript": "^5.4.2"
  },
  "type": "module",
  "dependencies": {
    "dotenv": "^16.4.5",
    "ts-node": "^10.9.2"
  }
}
